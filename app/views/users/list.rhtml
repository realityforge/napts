<table>
  <tr><th>User name</th></tr>
  <tr></tr>
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= link_to( "delete",
		     {:action => "delete", :id => user.id},
		     :confirm => "Really delete #{user.name}?") %>
      </td>
    </tr>
  <% end %>
</table>
<%= link_to('New User', :action => 'new' ) %>

