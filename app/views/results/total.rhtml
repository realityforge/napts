<table>
  <tr>
    <th>Quiz name</th>
    <th>Number correct answers</th>
    <th>Total number questions</th>
  </tr>
  <% for i in 0..@quiz_attempts.length %>
    <tr>
      <td><%= @quiz_name[i] %></td>
      <td><%= @numcorrect[i].to_s %></td>
      <td><%= @numqns[i].to_s %></td>
    </tr>
  <% end %>
</table>

<%= link_to( "back", :action => 'index' ) %>
